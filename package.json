  "main": "index.js",
  "scripts": {
    "dev": "APP_ENVIRONMENT=local next dev",
    "dev:windows": "set APP_ENVIRONMENT=local && next dev",
    "build": "next build",
    "build:prod": "APP_ENVIRONMENT=production npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:js": "eslint --ext .js --ext .ts --ext .tsx ./src",
    "fix": "npm run lint:js -- --fix && prettier-eslint --write",
    "prepare": "is-ci || husky install",
    "format": "prettier src --write",
    "typecheck": "tsc --noEmit --pretty",
    "imagemin": "node utils/runImagemin.mjs",
    "postbuild": "next export -o dist/ && next-sitemap"
  },
